									</div>
								</div>
								<!--div class="form-group">
									<label for="digitalObjectIdentifier" class="control-label col-md-5">DOI:</label>
									<div class="col-md-7">
										<t:textfield class="form-control input-sm" id="digitalObjectIdentifier" t:value="record.digitalObjectIdentifier"></t:textfield>
									</div>
								</div-->
							</div> <!-- column3 -->
		
							<button type="submit" class="btn btn-default">Update header</button>
						</t:form>
					</t:zone>
			</div> <!-- panelInfo -->
		</div> <!-- tabpanel -->
		
		<div role="tabpanel" class="tab-pane" id="tab2">
			<h2>Title Information</h2>
			
			<t:zone t:id="titleInfoZone" t:elementName="div">
				<t:if test="record.titleInfos" negate="true">
					<div class="alert alert-danger" role="alert">
						<span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
						At least one titleInfo element is required! Create one below.
					</div> <!-- alert -->
				</t:if>
				
				<t:loop source="record.titleInfos" t:value="titleInfo">
					<div class="panel panel-info">
						<div class="panel-heading">TitleInfo</div>
						<t:zone t:id="updateTitleInfoZone" t:elementname="div" class="panel-body">
							<t:form t:id="updateTitleInfo" t:object="titleInfo" t:context="titleInfo.id" t:include="title" class="form-horizontal" zone="^">
								<t:errors/>
								
								<t:zone t:id="typeZone">
								
								<t:if test="notFirstTitleInfo">
									<div class="form-group">
										<label for="ti_type" class="control-label col-md-2">Type:</label>
										<div class="col-md-8">
											<button class="btn btn-default dropdown-toggle btn-sm col-md-12 btnRight" type="button" id="dropdownMenuTiType" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
												${titleInfoTypeLabel} <span class="caret full"></span>
											</button>
											<ul class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuTiType">
												<li><t:actionlink t:id="setTypeTranslated" context="titleInfo.id" t:zone="^">Translated</t:actionlink></li>
												<li><t:actionlink t:id="setTypeAbbreviated" context="titleInfo.id" t:zone="^">Abbreviated</t:actionlink></li>
												<li><t:actionlink t:id="setTypeAlternative" context="titleInfo.id" t:zone="^">Alternative</t:actionlink></li>
												<li><t:actionlink t:id="setTypeUniform" context="titleInfo.id" t:zone="^">Uniform</t:actionlink></li>
												<li><t:actionlink t:id="setTypeBlank" context="titleInfo.id" t:zone="^">None</t:actionlink></li>
											</ul>
										</div>
												
									<t:if test="titleInfo.typeAbbreviated">
										<div class="dropdown col-md-2">
											<button class="btn btn-default dropdown-toggle btn-sm" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
												Add Authority <span class="caret"></span>
											</button>
											<ul class="dropdown-menu dropdown-menu-left" aria-labelledby="dropdownMenu1">
												<t:if test="!titleInfo.authoritydnlm">
													<li><t:actionlink t:id="addauthoritydnlm" context="titleInfo.id" t:zone="^">National Library of Medicine</t:actionlink></li>
												</t:if>
												<t:if test="!titleInfo.authorityhlasja">
													<li><t:actionlink t:id="addauthorityhlasja" context="titleInfo.id" t:zone="^">HLAS Journal Abbreviations</t:actionlink></li>
												</t:if>
												<t:if test="!titleInfo.authorityinisaljt">
													<li><t:actionlink t:id="addauthorityinisaljt" context="titleInfo.id" t:zone="^">INIS: Authority List for Journal Titles</t:actionlink></li>
												</t:if>
												<t:if test="!titleInfo.authorityissnkey">
													<li><t:actionlink t:id="addauthorityissnkey" context="titleInfo.id" t:zone="^">Abbreviated Key Titles</t:actionlink></li>
												</t:if>
											</ul>
										</div> <!-- dropdown -->
									</t:if>
										
									<t:if test="titleInfo.typeUniform">
										<div class="dropdown col-md-2">
											<button class="btn btn-default dropdown-toggle btn-sm" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
												Add Authority <span class="caret"></span>
											</button>
											<ul class="dropdown-menu dropdown-menu-left" aria-labelledby="dropdownMenu1">
												<t:if test="!titleInfo.authoritynaf">
													<li><t:actionlink t:id="addauthoritynaf" context="titleInfo.id" t:zone="^">Library of Congress Name Authority File</t:actionlink></li>
												</t:if>
												<t:if test="!titleInfo.authorityulan">
													<li><t:actionlink t:id="addauthorityulan" context="titleInfo.id" t:zone="^">Union List of Artist Names</t:actionlink></li>
												</t:if>
												<t:if test="!titleInfo.authorityunbisn">
													<li><t:actionlink t:id="addauthorityunbisn" context="titleInfo.id" t:zone="^">UNBIS Name Authority List</t:actionlink></li>
												</t:if>
												<t:if test="!titleInfo.authorityviaf">
													<li><t:actionlink t:id="addauthorityviaf" context="titleInfo.id" t:zone="^">Virtual International Authority File</t:actionlink></li>
												</t:if>
											</ul>
										</div> <!-- dropdown -->
									</t:if>
								</div> <!--form group-->
								
								<div class="row col-md-8 col-md-offset-2">
									<t:if test="titleInfo.authority">
										<div class="form-group">
											<label for="ti_abbreviated_authority" class="control-label col-md-2">Authority:</label>
												<div class="well well-sm col-md-10">
													${titleInfo.authority}	
												</div>
												<label for="ti_abbreviated_uri" class="control-label col-md-2">Authority URI:</label>
												<div class="well well-sm col-md-10">
													${titleInfo.uri}
												</div>
												<div class="form-group">
													<label for="ti_abbreviated_valueUri" class="control-label col-md-2">Value URI:</label>
													<div class="col-md-10">
														<t:textfield class="form-control input-sm" t:id="ti_abbreviated_valueUri" t:value="titleInfo.valueuri"></t:textfield>
													</div>
												</div> <!-- form-group -->
										</div> <!-- form-group -->
									</t:if>
								</div>	
										
								<div class="row col-md-8 col-md-offset-2">
									<t:if test="titleInfo.uniformauthority">
										<div class="form-group">
											<label for="ti_uniform_authority" class="control-label col-md-2">Authority:</label>
											<div class="well well-sm col-md-10">
												${titleInfo.uniformauthority}	
											</div>
											<label for="ti_uniform_uri" class="control-label col-md-2">Authority URI:</label>
											<div class="well well-sm col-md-10">
												${titleInfo.uniformuri}
											</div>
											<div class="form-group">
												<label for="ti_uniform_valueUri" class="control-label col-md-2">Value URI:</label>
												<div class="col-md-10">
													<t:textfield class="form-control input-sm" t:id="ti_uniform_valueUri" t:value="titleInfo.uniformvalueuri"></t:textfield>
												</div>
											</div> <!-- form-group -->	
										</div> <!-- form-group -->
									</t:if>
								</div>			
								
								</t:if> <!-- not first -->
							</t:zone> <!-- type -->	
										
							
							<div class="col-md-2 col-md-offset-10" />
											
							<t:zone t:id="displayLabelZone">
						
								<div class="form-group">
									<label for="ti_displaylabel" class="control-label col-md-2">Display Label:</label>
									<div class="col-md-8">
										<t:textfield class="form-control input-sm" t:id="ti_displaylabel" t:value="titleInfo.tidisplaylabel"></t:textfield>
									</div>
								</div> <!-- form-group -->
						
							</t:zone> <!-- displayLabel -->
							
							<div class="col-md-2 col-md-offset-10" />	
							
							<t:zone t:id="titleInfoAttributeZone">	
				
								<t:if test="titleInfo.tiUsage">
									<div class="form-group">
										<label for="ti_usage" class="control-label col-md-2">Usage:</label>
										<div class="col-md-8">
											<t:select class="form-control input-sm" t:id="ti_usage" t:model="literal:primary" t:value="titleInfo.tiUsage" />
										</div>
									</div> <!-- form-group -->
								</t:if>
								
								<t:if test="titleInfo.tisupplied">
									<div class="form-group">
										<label for="ti_supplied" class="control-label col-md-2">Supplied:</label>
										<div class="col-md-8">
											<t:select class="form-control input-sm" t:id="ti_supplied" t:model="literal:yes" t:value="titleInfo.tisupplied" />
										</div>
									</div> <!-- form-group -->
								</t:if>
											
								<t:if test="titleInfo.tinametitlegroup">
									<div class="form-group">
										<label for="ti_nametitlegroup" class="control-label col-md-2">NameTitleGroup:</label>
										<div class="col-md-8">
											<t:textfield class="form-control input-sm" t:id="ti_nametitlegroup" t:value="titleInfo.tinametitlegroup"></t:textfield>
										</div>
									</div> <!-- form-group -->
								</t:if>
											
								<t:if test="titleInfo.tialtRepGroup">
									<div class="form-group">
										<label for="ti_altRepGroup" class="control-label col-md-2">AltRepGroup:</label>
										<div class="col-md-8">
											<t:textfield class="form-control input-sm" t:id="ti_altRepGroup" t:value="titleInfo.tialtRepGroup"></t:textfield>
										</div>
										<div class="dropdown col-md-2">
											<button class="btn btn-default dropdown-toggle btn-sm" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
												<t:actionlink t:id="addaltformat" context="titleInfo.id" t:zone="^">Add AltFormat</t:actionlink>
											</button>
										</div>
									</div> <!-- form-group -->
								</t:if>
											
								<t:if test="titleInfo.tialtFormat">
									<div class="form-group">
										<label for="ti_altFormat" class="control-label col-md-2">AltFormat:</label>
										<div class="col-md-8">
											<t:textfield class="form-control input-sm" t:id="ti_altFormat" t:value="titleInfo.tialtFormat"></t:textfield>
										</div>
										<div class="dropdown col-md-2">
											<button class="btn btn-default dropdown-toggle btn-sm" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
												<t:actionlink t:id="addcontenttype" context="titleInfo.id" t:zone="^">Add ContentType</t:actionlink>
											</button>
										</div>
									</div> <!-- form-group -->
								</t:if>
								
								<t:if test="titleInfo.ticontentType">
									<div class="form-group">
										<label for="ti_contentType" class="control-label col-md-2">ContentType:</label>
										<div class="col-md-8">
											<t:textfield class="form-control input-sm" t:id="ti_contentType" t:value="titleInfo.ticontentType"></t:textfield>
										</div>
									</div> <!-- form-group -->
								</t:if>
											
								<t:if test="titleInfo.tixlink">
									<div class="form-group">
										<label for="ti_xlink" class="control-label col-md-2">Xlink:</label>
										<div class="col-md-8">
											<t:textfield class="form-control input-sm" t:id="ti_xlink" t:value="titleInfo.tixlink"></t:textfield>
										</div>
									</div> <!-- form-group -->
								</t:if>
											
								<t:if test="titleInfo.tiid">
									<div class="form-group">
										<label for="ti_tiid" class="control-label col-md-2">ID:</label>
										<div class="col-md-8">
											<t:textfield class="form-control input-sm" t:id="ti_tiid" t:value="titleInfo.tiid"></t:textfield>
										</div>
									</div> <!-- form-group -->
								</t:if>
								
							<t:if test="titleInfo.tiLang">
									<div class="form-group">
										<label for="ti_lang" class="control-label col-md-2">Lang:</label>
										<div class="col-md-8">
											<t:select class="form-control input-sm" t:id="ti_lang" t:model="literal:eng,fre,ger,gle,ita,lat,spa" t:value="titleInfo.tiLang" />
										</div>
									</div> <!-- form-group -->
								</t:if>
											
								<t:if test="titleInfo.tixmllang">
									<div class="form-group">
										<label for="ti_xmllang" class="control-label col-md-2">XML Lang:</label>
										<div class="col-md-8">
											<t:select class="form-control input-sm" t:id="ti_xmllang" t:model="literal:en,fr,de,ga,it,la,es" t:value="titleInfo.tixmllang" />
										</div>
									</div> <!-- form-group -->
								</t:if>
											
								<t:if test="titleInfo.tiscript">
									<div class="form-group">
										<label for="ti_script" class="control-label col-md-2">Script:</label>
										<div class="col-md-8">
											<t:select class="form-control input-sm" t:id="ti_script" t:model="literal:arab,brai,cyrl,goth,grek,hebr,jpan,latg,latn" t:value="titleInfo.tiscript" />
										</div>
									</div> <!-- form-group -->
								</t:if>
											
								<t:if test="titleInfo.titransliteration">
									<div class="form-group">
										<label for="ti_transliteration" class="control-label col-md-2">Transliteration:</label>
										<div class="col-md-8">
											<t:textfield class="form-control input-sm" t:id="ti_transliteration" t:value="titleInfo.titransliteration"></t:textfield>
										</div>
									</div> <!-- form-group -->
								</t:if>
											
								<t:if test="titleInfo.tiothertype">
									<div class="form-group">
										<label for="ti_othertype" class="control-label col-md-2">Other Type:</label>
										<div class="col-md-8">
											<t:textfield class="form-control input-sm" t:id="ti_othertype" t:value="titleInfo.tiothertype"></t:textfield>
										</div>
									</div> <!-- form-group -->
								</t:if>
							
							<div class="col-md-2 col-md-offset-10" />
							
							<div class="form-group">	
									<label for="ti_addattribute" class="control-label col-md-5">
									</label>
									<t:if test="remainingtitleinfoattributes">
									<div class="dropdown col-md-2">
										<button class="btn btn-default dropdown-toggle btn-sm" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
											Add TitleInfo Attribute <span class="caret"></span>
										</button>
										<ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
											<t:if test="!titleInfo.tiUsage">
												<li><t:actionlink t:id="addusage" context="titleInfo.id" t:zone="^">usage</t:actionlink></li>
											</t:if>
											<t:if test="!titleInfo.tisupplied">
												<li><t:actionlink t:id="addsupplied" context="titleInfo.id" t:zone="^">supplied</t:actionlink></li>
											</t:if>
											<t:if test="!titleInfo.tinametitlegroup">
												<li><t:actionlink t:id="addnametitlegroup" context="titleInfo.id" t:zone="^">nameTitleGroup</t:actionlink></li>
											</t:if>
											<t:if test="!titleInfo.tialtrepgroup">
												<li><t:actionlink t:id="addaltrepgroup" context="titleInfo.id" t:zone="^">altRepGroup</t:actionlink></li>
											</t:if>
											<t:if test="!titleInfo.tixlink">
												<li><t:actionlink t:id="addxlink" context="titleInfo.id" t:zone="^">xlink</t:actionlink></li>
											</t:if>
											<t:if test="!titleInfo.tiid">
												<li><t:actionlink t:id="addtiid" context="titleInfo.id" t:zone="^">ID</t:actionlink></li>
											</t:if>
											<t:if test="!titleInfo.tilang">
												<li><t:actionlink t:id="addlang" context="titleInfo.id" t:zone="^">lang</t:actionlink></li>
											</t:if>
											<t:if test="!titleInfo.tixmllang">
												<li><t:actionlink t:id="addxmllang" context="titleInfo.id" t:zone="^">xml:lang</t:actionlink></li>
											</t:if>
											<t:if test="!titleInfo.tiscript">
												<li><t:actionlink t:id="addscript" context="titleInfo.id" t:zone="^">script</t:actionlink></li>
											</t:if>
											<t:if test="!titleInfo.titransliteration">
												<li><t:actionlink t:id="addtransliteration" context="titleInfo.id" t:zone="^">transliteration</t:actionlink></li>
